{
    "name": "vite-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "format": "npx prettier --write . & npx prettier --write './dev/**/*.pug'",
        "build": "npm run format && vite build && npx prettier --write './build/**/*.html'",
        "preview": "vite preview"
    },
    "browserslist": [
        "last 4 version"
    ],
    "devDependencies": {
        "@prettier/plugin-pug": "^3.0.0",
        "autoprefixer": "^10.4.15",
        "prettier": "3.0.3",
        "pug": "^3.0.2",
        "sass": "^1.67.0",
        "vite": "^4.4.5",
        "vite-plugin-pug": "^0.3.2"
    },
    "dependencies": {
        "axios": "^1.5.0",
        "imask": "^7.1.3",
        "lightgallery": "^2.7.2",
        "normalize.css": "^8.0.1",
        "nouislider": "^15.7.1",
        "resize-observer-polyfill": "^1.5.1",
        "simplebar": "^6.2.5",
        "swiper": "^10.2.0",
        "vanilla-lazyload": "^17.8.4"
    }
}
